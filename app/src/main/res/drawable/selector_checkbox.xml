<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/checkbox_normal"
        android:state_pressed="true"/>
    <item android:drawable="@drawable/checkbox_checked"
        android:state_checked="true" android:state_enabled="true"/>
    <item android:drawable="@drawable/checkbox_disable"
        android:state_checked="true" android:state_enabled="false"/>
    <item android:drawable="@drawable/checkbox_normal"  android:state_selected="false"/>

</selector>